{
    "name": "axy/sourcemap",
    "type": "library",
    "description": "Work with JavaScript/CSS Source Map",
    "keywords": [
        "sourcemap",
        "Source Map",
        "JavaScript",
        "CSS"
    ],
    "homepage": "https://github.com/axypro/sourcemap",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Grigoriev",
            "email": "go.vasac@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "axy/errors": "^3.0.1",
        "axy/codecs-base64vlq": "^2.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~10.0.15",
        "squizlabs/php_codesniffer": "=3.7.1"
    },
    "autoload": {
        "psr-4": {
            "axy\\sourcemap\\": "src",
            "axy\\sourcemap\\tests\\": "tests"
        }
    },
    "archive": {
        "exclude": [
            "/tests/*",
            "/devops/*",
            "/doc/*",
            "/*.sh",
            "/*.xml.dist",
            "/CHANGELOG.md"
        ]
    }
}
